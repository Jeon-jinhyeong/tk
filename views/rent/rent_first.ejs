<style>
    .container-full {
      padding-top: 90px;
      padding-left: 260px;
      padding-right: 260px;
      width: 1400px;

      /*background : url(./img/test2.png);
    background-size: 1920px;*/
    }

    .section-title {
      font-size: 50px;
      
      line-height: 1.1;
      text-align: center;
      color: #00a8ff;
    }

    .join-form-container {
      padding: 57px 0px;
      width: 1000px;
      float: left;
      margin-right: 50px;
    }

    .panel-heading {
      font-size: 29px;
      line-height: 1.88;
      text-align: left;
      color: #333333;
      font-weight: bold;
    }

    input[type="text"] {
      padding-left: 20px;
      padding-right: 20px;
      height: 58px;
      border: solid 1px #bfbfbf;
      background-color: #ffffff;
      font-size: 28px;
      color: #333333;


      font-size: 28px;
      
      text-align: left;
      color: #333333;

      width: 100%;
    }

    .label-section {
      width: 48px;
      font-size: 28px;
      line-height: 60px;
      text-align: left;
      color: #333333;
      width: 209px;
      margin-top: 20px;
      float: left;
      white-space: 19px;
    }

    .input-section {
      display: inline-block;
      margin-top: 18px;
    }

    .panel-item {
      clear: both;
    }

    .panel {
      margin-bottom: 60px;
