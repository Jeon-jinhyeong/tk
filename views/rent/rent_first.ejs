  <body style="background : url('https://cdn.zeplin.io/5c022850b6aa8378fa9f6b2a/screens/F6086E83-63B0-4FD5-B148-542CE6B85876.png');
background-size: 1920px; height: 2726px;">
  <% include ../common/partials/nav %>

  <section class="container-full">
      <% include ../common/partials/process %>
      
      <form action="../api/user/join" method="POST" id="signupform" class="join-form-container" role="form">
        <div class="form-ctl-section">
          <!-- 차종 -->
          <section class="panel car">
            <div class="panel-heading">차량선택</div>
            <div class="panel-body">
              <span class="car-selection">
                <label>
                  <img id="damas_img" src="/img/selection-truck/damas-on.png">
                  <input type="radio" name="truckType" value="0" data-car-name="다마스" checked>
                </label>
              </span>
              <span class="car-selection">
                <label>
                  <img id="labbo_img" src="/img/selection-truck/labo-off.png">
                  <input type="radio" name="truckType" value="1" data-car-name="라보">
                </label>
              </span>
              <span class="car-selection last">
                <label>
                  <img id="porter_img" src="/img/selection-truck/porter-off.png">
                  <input type="radio" name="truckType" value="2" data-car-name="포터2">
                </label>
              </span>
            </div>
          </section>

          <!-- 지점 -->
          <section class="panel region">
            <div class="panel-heading">지점선택</div>
            <div class="panel-body">
              <div class="region-selection">
                <label>
                  <span>사당</span>
                  <input type="radio" name="region" value="0" checked>
                </label>
              </div><!--
              --><div class="region-selection">
                <label>
                  <span>딜리버리 서비스</span>
                  <input type="radio" name="region" value="1">
                </label>
              </div>
            </div>
            
          </section>

          <!-- 날짜 -->
          <section class="panel">
            <div class="panel-heading">날짜선택</div>
            <div class="panel-body">

              <input type="text" id="rent_time" disabled>
              <div id="rent_datetimepicker"></div>

              <input type="text" id="end_time" disabled>
              <div id="end_datetimepicker"></div>
            </div>
          </section>

          <!-- 약관 정보 -->
          <section>
            <h3>예약완료를 위한 이용자 동의 사항</h3>
            <div>
              <label>
                <input type="checkbox" name=""> 예약완료를 위한 이용 동의(필수), 차량 대여를 위한 개인정보 수집/이용 동의(필수), 개인정보 취급 위탁 동의(필수)에 모두 동의합니다.
              </label>
            </div>
          </section>
          
          <button id="btn-signup" class="next" type="submit">다음단계</button>
        </div>
      </form>
    
      <% include ../common/partials/rent-result %>
      </div>
    </section>

