      line-height: 1.79;
      text-align: left;
      color: #333333;
    }

    #show_select_car_section {
      width: 300px;
      height: 80px;
      background-color: #00a8ff;
      font-size: 28px;
      font-weight: bold;
      line-height: 1.47;
      text-align: center;
      color: #ffffff;
    }
    
