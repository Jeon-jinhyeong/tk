      float: left;
      width: 350px;
      margin-top: 70px;
    }

    h3 {
      font-size: 24px;
      
      /* line-height: 2.08; */
      text-align: left;
      color: #ffffff;
      background: #6b879e;
      padding: 22px 24px;
    }

    

    .process {
      width: 466px;
      height: 88px;
      display: inline-block;
      background-color: #d2d2d2;
      font-size: 30px;
      
      line-height: 3;
      text-align: center;
      color: #ffffff;
    }

    .process.active {
      background-color: #00a8ff !important;
    }

    .payment-select-section {
      background-color: #f9f9f9;
      padding: 40px;
    }

    .payment-select-section .label-section{
      width: 184px;
      font-size: 28px;
      
      line-height: 1.79;
      text-align: left;
      color: #333333;
